<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.columbia.questnyc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SignInActivity"
            android:label="@string/title_activity_sign_in" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="HelpActivity"></activity>
        <activity android:name="com.columbia.location.BoroughActivity"></activity>
        <activity android:name="com.columbia.location.NeighborhoodActivity"></activity>
        <activity android:name="com.columbia.quest.QuestActivity"></activity>
        <activity android:name="com.columbia.quest.QuestionActivity"></activity>
        <activity android:name="com.columbia.quest.RatingActivity"></activity>
        <activity android:name="com.columbia.quest.create.CreateQuestActivity"></activity>
        </application>

</manifest>